{
    "extends": ["prettier"],
    "plugins": ["prettier", "react", "react-hooks", "eslint-plugin-import-helpers"],
    "rules": {
      "prettier/prettier": ["error"],
      "prefer-destructuring": [
        "error",
        {
          "array": true,
          "object": true
        },
        {
          "enforceForRenamedProperties": false
        }
      ],
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
      "no-console": "error"
    }
  }
